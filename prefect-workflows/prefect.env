# Prefect Configuration for Project Genji
# This file contains environment-specific settings for Prefect

# Prefect API Settings
PREFECT_API_URL=http://localhost:4200/api
PREFECT_SERVER_API_HOST=0.0.0.0
PREFECT_SERVER_API_PORT=4200
PREFECT_UI_URL=http://localhost:4200

# Logging Configuration
PREFECT_LOGGING_LEVEL=INFO
PREFECT_LOGGING_SETTINGS_PATH=

# Task Runner Settings
PREFECT_TASK_RUNNER=ThreadPoolTaskRunner
PREFECT_TASK_RUNNER_MAX_WORKERS=5

# Flow Run Settings
PREFECT_FLOW_RUN_TIMEOUT_SECONDS=3600

# Database Settings (inherits from main .env)
# These will be loaded from python-analysis-module/.env

# Notification Settings (optional)
PREFECT_NOTIFICATIONS_ENABLED=true
SLACK_WEBHOOK_URL=
EMAIL_SMTP_SERVER=
EMAIL_SMTP_PORT=587
EMAIL_USERNAME=
EMAIL_PASSWORD=

# Security Settings
PREFECT_CLOUD_API_KEY=
PREFECT_API_KEY=

# Performance Settings
PREFECT_RESULTS_PERSIST_BY_DEFAULT=true
PREFECT_LOCAL_STORAGE_PATH=/tmp/prefect
